using System;

namespace Basketball_Equipment
{
	class Program
	{
		static void Main(string[] args)
		{
			double tax = double.Parse(Console.ReadLine());

			double shoes = tax  * 0.6;
			double ekip = shoes * 0.8;
			double boll = ekip / 4;
			double accecoar = boll / 5;
			double total = shoes + ekip + boll + accecoar+ tax;

			Console.WriteLine($"{total:f2}");
		}
	}
}
