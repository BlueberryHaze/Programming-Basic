using System;

namespace Building
{
	class Program
	{
		static void Main(string[] args)
		{
			int floorCount = int.Parse(Console.ReadLine());
			int roomPerFloorCount = int.Parse(Console.ReadLine());

			for (int floorNumber = floorCount; floorNumber >= 1; floorNumber--)
			{
				for (int roomNumber = 0; roomNumber < roomPerFloorCount; roomNumber++)
				{
					if (floorNumber == floorCount)
					{
						Console.Write($"L{floorNumber}{roomNumber} ");
					}
					else if (floorNumber % 2 == 0)
					{
						Console.Write($"O{floorNumber}{roomNumber} ");
					}
					else
					{
						Console.Write($"A{floorNumber}{roomNumber} ");

					}
				}
				Console.WriteLine();
			}
		}
	}
}
